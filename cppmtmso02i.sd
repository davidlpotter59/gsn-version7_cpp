701     begin
        if sfpmaster:trans_code = 14 then
            begin
            l_distance_to_shore = sfplocation:distance_to_shore
            l_state = cppgeneral:state
            access sfsdistance, set sfsdistance:company_id        = sfpname:company_id,
                                    sfsdistance:state             = l_state,
                                    sfsdistance:line_of_business  = sfpname:line_of_business,
                                    sfsdistance:distance_to_shore = l_distance_to_shore, generic

            l_special_exposure_factor = 1.00
            while sfsdistance:company_id        = sfpname:company_id and
                  sfsdistance:state             = l_state and
                  sfsdistance:line_of_business  = sfpname:line_of_business and
                  sfsdistance:distance_to_shore = l_distance_to_shore
                begin
                if (sfpname:eff_date >= sfsdistance:EfF_date and
                   (sfpname:eff_date <= sfsdistance:exp_date or
                   sfsdistance:exp_date = 0)) then
                    l_special_exposure_factor = sfsdistance:factor

                next sfsdistance
                end

            if l_special_exposure_factor <> 1.00 then
                cppgeneral:special_exposure = 1
            else
                cppgeneral:special_exposure = 0
            cppgeneral:special_exposure_factor = l_special_exposure_factor
            end
        end
        cppgeneral:special_exposure/checkbox/values="0,1"/default
        hide if cppgeneral:state not one of 7, 29
        if cppgeneral:special_exposure = 0 then
            cppgeneral:special_exposure_factor = 1.00 ;
