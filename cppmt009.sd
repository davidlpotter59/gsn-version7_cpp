Update
If UPDATEOK = "YES" And
   Abort = "NO" Then
    Begin
    if function one of "DELETE" then
        begin
        access sfpend, set sfpend:policy_no    = cppartisan:policy_no,
                           sfpend:pol_year     = cppartisan:pol_year,
                           sfpend:end_sequence = cppartisan:end_sequence,
                           sfpend:prem_no      = cppartisan:prem_no,
                           sfpend:build_no     = cppartisan:build_no, generic

        while sfpend:policy_no    = cppartisan:policy_no and
              sfpend:pol_year     = cppartisan:pol_year and
              sfpend:end_sequence = cppartisan:end_sequence and
              sfpend:prem_no      = cppartisan:prem_no and
              sfpend:build_no     = cppartisan:build_no
                begin
                if sfpend:lob_end_code one of "R" then
                    delete sfpend

                next sfpend
                end

        end

    if function one of "ADD", "NEW", "CHANGE", "CHG" then
        begin
        l_state = cppartisan:state
        Access CPSLIABTRIGGER, Set CPSLIABTRIGGER:COMPANY_ID = sfpNAME:COMPANY_ID,
                                   CPSLIABTRIGGER:STATE = L_STATE,
                                   CPSLIABTRIGGER:LINE_OF_BUSINESS = L_LINE_OF_BUSINESS, Generic

        While CPSLIABTRIGGER:COMPANY_ID = sfpNAME:COMPANY_ID And
              CPSLIABTRIGGER:STATE = L_STATE And
              CPSLIABTRIGGER:LINE_OF_BUSINESS = L_LINE_OF_BUSINESS
                Begin
                If sfpNAME:EFF_DATE >= CPSLIABTRIGGER:EFF_DATE Then
                    Begin
                    l_code_1 = cpsliabtrigger:trigger_endorsement[2]
                    l_code_2 = cpsliabtrigger:trigger_endorsement[3]
                    End

                Next CPSLIABTRIGGER
                End

        If L_CODE_1 <> "" Then
            Begin
            L_CODE = L_CODE_1
            Do ACCESS_sfpEND_1
            If L_ADD_ENDORSEMENT = "Y" And
               sfpname:bill_plan = "DB" Then
                Do ADD_sfpEND_1
            If L_ADD_ENDORSEMENT = "N" And
               sfpname:bill_plan not one of "DB" Then
                Do DELETE_sfpEND_1
            End

        If L_CODE_2 <> "" Then
            Begin
            L_CODE = L_CODE_2
            Do ACCESS_sfpEND_1
            If L_ADD_ENDORSEMENT = "Y" And
               cppartisan:class_code >= 90000 and
               cppartisan:class_code <= 99999 Then
                Do ADD_sfpEND_1
            If ((L_ADD_ENDORSEMENT = "N" And
               cppartisan:class_code < 90000) or
               (l_add_endorsement = "N" and
               cppartisan:class_code > 99999)) Then
                Do DELETE_sfpEND_1
            End

        end

    if function one of "ADD", "NEW" then
        begin
        Access cpppolicy, Set cpppolicy:policy_NO = cppartisan:policy_NO,
                              cpppolicy:pol_year = cppartisan:pol_Year,
                              cpppolicy:end_sequence = cppartisan:end_sequence,
                              cpppolicy:PREM_NO = cppartisan:PREM_NO,
                              cpppolicy:BUILD_NO = cppartisan:BUILD_NO, Generic

        If cpppolicy:policy_NO <> cppartisan:policy_NO Or
           cpppolicy:pol_year <> cppartisan:pol_year or
           cpppolicy:end_sequence <> cppartisan:end_sequence or
           cpppolicy:PREM_NO <> cppartisan:PREM_NO Or
           cpppolicy:BUILD_NO <> cppartisan:BUILD_NO Then
            Add cpppolicy
                Begin
                cpppolicy:policy_NO = cppartisan:policy_NO
                cpppolicy:pol_year = cppartisan:pol_year
                cpppolicy:end_sequence = cppartisan:end_sequence
                cpppolicy:PREM_NO = cppartisan:PREM_NO
                cpppolicy:BUILD_NO = cppartisan:BUILD_NO
                End
        end

    If FUNCTION one of "ADD", "NEW" Then
        Begin
        access cppartdeviation, set cppartdeviation:policy_no = g_policy_no,
                                    cppartdeviation:pol_year = g_Year,
                                    cppartdeviation:end_sequence = g_end_sequence, approximate

        if cppartdeviation:policy_no <> g_policy_no or
           cppartdeviation:pol_Year <> g_year or
           cppartdeviation:end_sequence <> g_end_sequence or
           sfscompany:boiler_rating not one of 1 then
            begin
            G_policy_NO = cppartisan:policy_NO
            g_year = cppartisan:pol_year
            g_end_sequence = cppartisan:end_sequence
            G_STATE = cppartisan:STATE
            g_popup = "Y"
            if cppartdeviation:policy_no = g_policy_no and
               cppartdeviation:pol_Year = g_year and
               cppartdeviation:end_sequence = g_end_sequence then
                begin
                Display Modal Screen "cppmt998"
                With Initial Function "CHANGE|BACK"
                end
            else
                begin
                display modal screen "cppmt998"
                with initial function "ADD|BACK"
                end
            g_popup = "N"
            end

        Access cppartdeduct, Set cppartdeduct:policy_NO = G_policy_NO,
                                 cppartdeduct:pol_Year = g_year,
                                 cppartdeduct:end_sequence = g_end_sequence, generic

        If cppartdeduct:policy_NO <> G_policy_NO or
           cppartdeduct:pol_year <> g_year or
           cppartdeduct:end_sequence <> g_end_sequence then
            Begin
            do access_cpsliabmiscrates
            G_STATE = cppartisan:STATE
            if l_deductible <> 0 then
                begin
                Display Modal Screen "cppmt954"
                with initial function "CHANGE|BACK"
                end
            else
                begin
                Display Modal Screen "cppmt954"
                With Initial Function "ADD|BACK"
                end
            End

        End

    End

Screen Exit
        If cppartisan:PREM_NO > 0 Then G_PREM_NO = cppartisan:PREM_NO ;
        If cppartisan:BUILD_NO > 0 Then G_BUILD_NO = cppartisan:BUILD_NO ;
        If cppartisan:CLASS_CODE > 0 Then G_CLASS_CODE = cppartisan:CLASS_CODE ;
  g_prem_no_save = g_prem_no
  g_build_no_save = g_build_no
