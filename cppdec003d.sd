   Set cppartisan:POLICY_NO    = SFPPRINT1:POLICY_NO,
       cppartisan:POL_YEAR     = SFPPRINT1:POL_YEAR,
       cppartisan:END_SEQUENCE = SFPPRINT1:END_SEQUENCE, Generic

