   Set cppartlimits:POLICY_NO    = SFPPRINT1:POLICY_NO,
       cppartlimits:POL_YEAR     = SFPPRINT1:POL_YEAR,
       cppartlimits:END_SEQUENCE = SFPPRINT1:END_SEQUENCE, Generic

