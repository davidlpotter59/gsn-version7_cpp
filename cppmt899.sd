                                   cppliability_alias:class_code = cppliability:class_code, generic

    change cppliability_alias
        begin
        cppliability_alias:prem[2]       = l_products_prem
        end
    end
else
    begin
    access cppliability_alias, set cppliability_alias:policy_no   = cppliability:policy_no,
                                   cppliability_alias:pol_year     = cppliability:pol_year,
                                   cppliability_alias:end_sequence = cppliability:end_sequence,
                                   cppliability_alias:prem_no    = cppliability:prem_no,
                                   cppliability_alias:build_no   = cppliability:build_no,
                                   cppliability_alias:class_code = cppliability:class_code, generic

    change cppliability_alias
        begin
        cppliability_alias:prem[2]       = 0
        end
    end

l_wide_end_premium = 0
if l_first_end = "Y" then
    begin
    l_first_End = "N"
    Access sfpEND, Set sfpEND:policy_NO = cppliability:policy_NO,
                       sfpend:pol_year = cppliability:pol_year,
                       sfpend:end_Sequence = cppliability:end_sequence,
                       sfpend:prem_no  = 0,
                       sfpend:build_no = 0, generic

    while sfpEND:policy_NO = cppliability:policy_NO  and
          sfpend:pol_year = cppliability:pol_year and
          sfpend:end_sequence = cppliability:end_sequence and
          sfpend:prem_no  = 0 and
          sfpend:build_no = 0
        begin
        if sfpend:lob_end_code one of "L" then
        begin
        L_UPDATE_SFPEND_PREMIUM  = 0
        L_CODE                   = sfpEND:CODE
        if sfpend:manual_premium <> 0 then
            l_update_sfpend_premium = sfpend:manual_premium
        else
            begin
            Do RATE_ENDORSEMENTS

            if l_update_sfpend_premium <> 0 then
                begin
                Change sfpEND
                    Begin
                    sfpEND:PREMIUM = L_UPDATE_SFPEND_PREMIUM
                    end
                End
            end

        L_wide_END_PREMIUM = L_wide_END_PREMIUM + L_UPDATE_SFPEND_PREMIUM
        end

        Next sfpEND
        End

    end

l_end_premium = 0
if cppliability:prem_no  <> l_prem_no or
   cppliability:build_no <> l_build_no then
    begin
    Access sfpEND, Set sfpEND:policy_NO = cppliability:policy_NO,
                       sfpend:pol_year = cppliability:pol_year,
                       sfpend:end_Sequence = cppliability:end_sequence,
                       sfpend:prem_no  = cppliability:prem_no,
                       sfpend:build_no = cppliability:build_no,  generic

    while sfpEND:policy_NO = cppliability:policy_NO and
          sfpend:pol_year = cppliability:pol_year and
          sfpend:end_sequence = cppliability:end_sequence and
          sfpend:prem_no  = cppliability:prem_no and
          sfpend:build_no = cppliability:build_no
        begin
        if sfpend:lob_end_code one of "L" then
        begin
        L_UPDATE_SFPEND_PREMIUM  = 0
        L_CODE                   = sfpEND:CODE
        if sfpend:manual_premium <> 0 then
            l_update_sfpend_premium = sfpend:manual_premium
        else
            begin
            Do RATE_ENDORSEMENTS

            if l_update_sfpend_premium <> 0 then
                begin
                Change sfpEND
                    Begin
                    sfpEND:PREMIUM = L_UPDATE_SFPEND_PREMIUM
                    end
                End
            end

        L_END_PREMIUM = L_END_PREMIUM + L_UPDATE_SFPEND_PREMIUM
        end

        Next sfpEND
        End
    end

if cppliability:prem_no  <> l_prem_no or
   cppliability:build_no <> l_build_no then
    begin
    access cppliability_alias, set cppliability_alias:policy_no    = cppliability:policy_no,
                                   cppliability_alias:pol_year     = cppliability:pol_year,
                                   cppliability_alias:end_sequence = cppliability:end_sequence,
                                   cppliability_alias:prem_no      = cppliability:prem_no,
                                   cppliability_alias:build_no     = cppliability:build_no,
                                   cppliability_alias:class_code   = cppliability:class_code, generic

    change cppliability_alias
        begin
        cppliability_alias:total_endorsements = l_end_premium +
                                                l_wide_end_premium
        end
    end
else
if cppliability:prem_no  = l_prem_no and
   cppliability:build_no = l_build_no then
    begin
    access cppliability_alias, set cppliability_alias:policy_no    = cppliability:policy_no,
                                   cppliability_alias:pol_year     = cppliability:pol_year,
                                   cppliability_alias:end_sequence = cppliability:end_sequence,
                                   cppliability_alias:prem_no      = cppliability:prem_no,
                                   cppliability_alias:build_no     = cppliability:build_no,
                                   cppliability_alias:class_code   = cppliability:class_code, generic

    change cppliability_alias
        begin
        cppliability_alias:total_endorsements = 0
        end
    end

end


Procedure fire_legal
Begin
Access CPSLIABCOVMOD, Set CPSLIABCOVMOD:COMPANY_ID       = sfpNAME:COMPANY_ID ,
                          CPSLIABCOVMOD:STATE            = cppliability:STATE,
                          CPSLIABCOVMOD:LINE_OF_BUSINESS = cppliability:rating_LINE_OF_BUSINESS, Generic

While CPSLIABCOVMOD:COMPANY_ID       = sfpNAME:COMPANY_ID And
      CPSLIABCOVMOD:STATE            = cppliability:STATE And
      CPSLIABCOVMOD:LINE_OF_BUSINESS = cppliability:rating_LINE_OF_BUSINESS
        Begin
        If sfpNAME:EFF_DATE >= CPSLIABCOVMOD:EFF_DATE Then
            Begin
            if cppliablimits:fire_legal = cpsliabcovmod:fire_legal_limit[1] then
                l_fire_legal_charge = cpsliabcovmod:fire_legal_charge[1]
            if cppliablimits:fire_legal = cpsliabcovmod:fire_legal_limit[2] then
                l_fire_legal_charge = cpsliabcovmod:fire_legal_charge[2]
            if cppliablimits:fire_legal = cpsliabcovmod:fire_legal_limit[3] then
                l_fire_legal_charge = cpsliabcovmod:fire_legal_charge[3]
            if cppliablimits:fire_legal = cpsliabcovmod:fire_legal_limit[4] then
                l_fire_legal_charge = cpsliabcovmod:fire_legal_charge[4]
            if cppliablimits:fire_legal = cpsliabcovmod:fire_legal_limit[5] then
                l_fire_legal_charge = cpsliabcovmod:fire_legal_charge[5]
            End

        Next CPSLIABCOVMOD
        End

    access cppliability_alias, set cppliability_alias:policy_no    = cppliability:policy_no,
                                   cppliability_alias:pol_year     = cppliability:pol_year,
                                   cppliability_alias:end_sequence = cppliability:end_sequence,
                                   cppliability_alias:prem_no      = cppliability:prem_no,
                                   cppliability_alias:build_no     = cppliability:build_no,
                                   cppliability_alias:class_code   = cppliability:class_code, generic

    change cppliability_alias
        begin
        cppliability_alias:fire_legal_premium = l_fire_legal_charge
        end
End

procedure prem_ops
begin
l_products = 0
l_premise_limits_factor = 0
do increased_limits
access cppliabdeduct, set cppliabdeduct:policy_no = cppliability:policy_no,
                          cppliabdeduct:pol_year     = cppliability:pol_year,
                          cppliabdeduct:end_sequence = cppliability:end_sequence, generic

if cppliabdeduct:deductible[1] <> 0 then
    begin
    l_deductible = cppliabdeduct:deductible[1]
    l_deductible_type = "1 "
    end
if cppliabdeduct:deductible[2] <> 0 then
    begin
    l_deductible = cppliabdeduct:deductible[2]
    l_deductible_type = "1 "
    end
if cppliabdeduct:deductible[3] <> 0 then
    begin
    l_deductible = cppliabdeduct:deductible[3]
    l_deductible_type = "2 "
    end
if cppliabdeduct:deductible[4] <> 0 then
    begin
    l_deductible = cppliabdeduct:deductible[4]
    l_deductible_type = "2 "
    end
if cppliabdeduct:deductible[5] <> 0 then
    begin
    l_deductible = cppliabdeduct:deductible[5]
    l_deductible_type = "3 "
    end
if cppliabdeduct:deductible[6] <> 0 then
    begin
    l_deductible = cppliabdeduct:deductible[6]
    l_deductible_type = "3 "
    end
do premise_deductible
do premise_mp

l_adj_rate = (((cppliability:base_rate[1] *
             l_exclusions) *
             (l_premise_limits_factor -
             l_deductible_factor_1)) *
             l_rmf)
l_prem_ops = (l_adj_rate *
             (cppliability:exposure[1] divide
             cppliability:rates_per[1]))

access cppliability_alias, set cppliability_alias:policy_no   = cppliability:policy_no,
                               cppliability_alias:pol_year     = cppliability:pol_year,
                               cppliability_alias:end_sequence = cppliability:end_sequence,
                               cppliability_alias:prem_no    = cppliability:prem_no,
                               cppliability_alias:build_no   = cppliability:build_no,
                               cppliability_alias:class_code = cppliability:class_code, generic

change cppliability_alias
    begin
    cppliability_alias:adjusted_rate[1]          = l_adj_rate
    cppliability_alias:prem[1]                   = l_prem_ops
    cppliability_alias:increase_limits_factor[1] = l_premise_limits_factor
    cppliability_alias:aggregate_surcharge[1]    = l_exclusions
    cppliability_alias:minimum_premium[1]        = l_prem_ops_mp
    cppliability_alias:deductible_factor[1]      = l_deductible_factor_1
    end

end

procedure products
begin
l_products = 1
l_products_limits_factor = 0
do increased_limits
access cppliabdeduct, set cppliabdeduct:policy_no = cppliability:policy_no,
                          cppliabdeduct:pol_year     = cppliability:pol_year,
                          cppliabdeduct:end_sequence = cppliability:end_sequence, generic

if cppliabdeduct:deductible[1] <> 0 then
    begin
    l_deductible = cppliabdeduct:deductible[1]
    l_deductible_type = "4 "
    end
if cppliabdeduct:deductible[2] <> 0 then
    begin
    l_deductible = cppliabdeduct:deductible[2]
    l_deductible_type = "4 "
    end
if cppliabdeduct:deductible[3] <> 0 then
    begin
    l_deductible = cppliabdeduct:deductible[3]
    l_deductible_type = "5 "
    end
if cppliabdeduct:deductible[4] <> 0 then
    begin
    l_deductible = cppliabdeduct:deductible[4]
