procedure exclusions
begin
access cpsliabexcl, set cpsliabexcl:company_id       = sfpname:company_Id,
                        cpsliabexcl:state            = cppliability:state,
                        cpsliabexcl:line_of_business = cppliability:rating_line_of_business, generic

while cpsliabexcl:company_id       = sfpname:company_id and
      cpsliabexcl:state            = cppliability:state and
      cpsliabexcl:line_of_business = cppliability:rating_line_of_business
        begin
        if (sfpname:eff_date >= cpsliabexcl:eff_date and
           (sfpname:eff_date <= cpsliabexcl:exp_date or
           cpsliabexcl:exp_date = 0)) then
            begin
            if cppliablimits:products = 0 then
                l_products_exclusion    = cpsliabexcl:exclusion_factor[1]
            if cppliablimits:advertising_personal_injury = 0 then
                l_advertising_exclusion = cpsliabexcl:exclusion_factor[4]
            if cppliablimits:fire_legal = 0 then
                l_fire_legal_exclusion  = cpsliabexcl:exclusion_factor[3]
            if cppliablimits:medical_payments = 0 then
                l_medical_exclusion     = cpsliabexcl:exclusion_factor[5]
            if cppliablimits:contract_coverage[1] = 1 then
                l_contract_exclusion    = cpsliabexcl:exclusion_factor[2]
            end

        next cpsliabexcl
        end

l_exclusions = ((100 -
               (l_products_exclusion +
               l_advertising_exclusion +
               l_fire_legal_exclusion +
               l_medical_exclusion +
               l_contract_exclusion)) divide
               100)
end

procedure premise_mp
begin
access cpsliabspecmp, set cpsliabspecmp:company_id       = sfpname:company_id,
                          cpsliabspecmp:state            = cppliability:state,
                          cpsliabspecmp:line_of_business = cppliability:rating_line_of_business,
                          cpsliabspecmp:class_code       = cppliability:class_code, generic

l_special_mp     = 0
l_no_products_mp = 0
while cpsliabspecmp:company_id       = sfpname:company_id and
      cpsliabspecmp:state            = cppliability:state and
      cpsliabspecmp:line_of_business = cppliability:rating_line_of_business and
      cpsliabspecmp:class_code       = cppliability:class_code
        begin
        if (sfpname:eff_date >= cpsliabspecmp:eff_date and
           (sfpname:eff_date <= cpsliabspecmp:exp_date or
           cpsliabspecmp:exp_date = 0)) then
            begin
            l_special_mp     = cpsliabspecmp:minimum_premium
            l_no_products_mp = cpsliabspecmp:no_products_mp

            end

        next cpsliabspecmp
        end

if l_special_mp = 0 then
    begin
    access cpsliabisomp, set cpsliabisomp:company_id       = sfpname:company_id,
                             cpsliabisomp:state            = cppliability:state,
                             cpsliabisomp:line_of_business = cppliability:rating_line_of_business, generic

    while cpsliabisomp:company_id       = sfpname:company_id and
          cpsliabisomp:state            = cppliability:state and
          cpsliabisomp:line_of_business = cppliability:rating_line_of_business
            begin
            if (sfpname:eff_date >= cpsliabisomp:eff_date and
               (sfpname:eff_date <= cpsliabisomp:exp_date or
               cpsliabisomp:exp_date = 0)) then
                begin
                switch(cppliability:limits_table)
                  case 1  : l_prem_ops_mp = cpsliabisomp:prem_ops[1]
                  case 2  : l_prem_ops_mp = cpsliabisomp:prem_ops[2]
                  default : l_prem_ops_mp = cpsliabisomp:prem_ops[3]
                  end
                end

                if l_no_products_mp = 1 then
                    l_prem_ops_mp = cpsliabisomp:combined_minimum_premium

            next cpsliabisomp
            end
    end

if l_special_mp = 0 then
    begin
    l_prem_ops_mp = (l_prem_ops_mp *
                    (l_premise_limits_factor -
                    l_deductible_factor_1) *
                    l_loss_cost)
    end
else
    begin
    l_prem_ops_mp = (l_special_mp *
                    (l_premise_limits_factor -
                    l_deductible_factor_1) *
                    l_loss_cost)
    end

end

procedure products_mp
begin
access cpsliabspecmp, set cpsliabspecmp:company_id       = sfpname:company_id,
                          cpsliabspecmp:state            = cppliability:state,
                          cpsliabspecmp:line_of_business = cppliability:rating_line_of_business,
                          cpsliabspecmp:class_code       = cppliability:class_code, generic

l_special_mp     = 0
l_no_products_mp = 0
while cpsliabspecmp:company_id       = sfpname:company_id and
      cpsliabspecmp:state            = cppliability:state and
      cpsliabspecmp:line_of_business = cppliability:rating_line_of_business and
      cpsliabspecmp:class_code       = cppliability:class_code
        begin
        if (sfpname:eff_date >= cpsliabspecmp:eff_date and
           (sfpname:eff_date <= cpsliabspecmp:exp_date or
           cpsliabspecmp:exp_date = 0)) then
            begin
            l_special_mp     = cpsliabspecmp:minimum_premium
            l_no_products_mp = cpsliabspecmp:no_products_mp

            end

        next cpsliabspecmp
        end

if l_special_mp = 0 then
    begin
    access cpsliabisomp, set cpsliabisomp:company_id       = sfpname:company_id,
                             cpsliabisomp:state            = cppliability:state,
                             cpsliabisomp:line_of_business = cppliability:rating_line_of_business, generic

    while cpsliabisomp:company_id       = sfpname:company_id and
          cpsliabisomp:state            = cppliability:state and
          cpsliabisomp:line_of_business = cppliability:rating_line_of_business
            begin
            if (sfpname:eff_date >= cpsliabisomp:eff_date and
               (sfpname:eff_date <= cpsliabisomp:exp_date or
               cpsliabisomp:exp_date = 0)) then
                begin
                switch(cppliability:products_limit_table)
                  case "A" : l_products_mp = cpsliabisomp:products[1]
                  case "B" : l_products_mp = cpsliabisomp:products[2]
                  case "C" : l_products_mp = cpsliabisomp:products[3]
                  end
                end

            next cpsliabisomp
            end
    end

if l_special_mp = 0 then
    begin
    l_products_mp = (l_products_mp *
                    (l_products_limits_factor -
                    l_deductible_factor_2) *
                    l_loss_cost)
    end
else
    begin
    l_products_mp = (l_special_mp *
                    (l_products_limits_factor -
                    l_deductible_factor_2) *
                    l_loss_cost)
    end

if l_no_products_mp = 1 then
    l_products_mp = 0
end

procedure increased_limits
begin
l_occurrence = cppliablimits:occurrence
l_aggregate  = cppliablimits:general_aggregate
access cpsliabinclimits, set cpsliabinclimits:company_id        = sfpname:company_id,
                             cpsliabinclimits:state             = cppliability:state,
                             cpsliabinclimits:line_of_business  = cppliability:rating_line_of_business,
                             cpsliabinclimits:products          = l_products,
                             cpsliabinclimits:occurrence        = l_occurrence,
                             cpsliabinclimits:general_aggregate = l_aggregate, generic

while cpsliabinclimits:company_id        = sfpname:company_id and
      cpsliabinclimits:state             = cppliability:state and
      cpsliabinclimits:line_of_business  = cppliability:rating_line_of_business and
      cpsliabinclimits:products          = l_products and
      cpsliabinclimits:occurrence        = l_occurrence and
      cpsliabinclimits:general_aggregate = l_aggregate
        begin
        if sfpname:eff_date >= cpsliabinclimits:eff_date then
            begin
            if l_products = 0 then
                begin
                switch(cppliability:limits_table)
                  case 1  : l_premise_limits_factor = cpsliabinclimits:factor[1]
                  case 2  : l_premise_limits_factor = cpsliabinclimits:factor[2]
                  default : l_premise_limits_factor = cpsliabinclimits:factor[3]
                  end
                end
            else
                begin
                switch(cppliability:products_limit_table)
                  case "A" : l_products_limits_factor = cpsliabinclimits:factor[1]
                  case "B" : l_products_limits_factor = cpsliabinclimits:factor[2]
                  default  : l_products_limits_factor = cpsliabinclimits:factor[3]
                  end
                end
            end

        next cpsliabinclimits
        end

end

procedure loss_cost
begin
l_lob = 0002
access sfslosscost, set sfslosscost:company_id       = sfpname:company_id,
                        sfslosscost:state            = cppliability:state,
                        sfslosscost:line_of_business = l_lob, generic

l_loss_cost = 0
while sfslosscost:company_id       = sfpname:company_id and
      sfslosscost:state            = cppliability:state and
      sfslosscost:line_of_business = l_lob
        begin
        if sfpname:eff_date >= sfslosscost:eff_date then
            l_loss_cost = sfslosscost:rate

        next sfslosscost
        end

l_rmf = l_loss_cost * cppliability:company_deviation[1] *
        cppliability:package_mod[1]
end

procedure premise_deductible
begin
access cpsliabdeduct, set cpsliabdeduct:company_id       = sfpname:company_id,
                          cpsliabdeduct:state            = cppliability:state,
                          cpsliabdeduct:line_of_business = cppliability:rating_line_of_business,
                          cpsliabdeduct:deductible       = l_deductible,
                          cpsliabdeduct:deductible_type  = l_deductible_type, generic

while cpsliabdeduct:company_id       = sfpname:company_id and
      cpsliabdeduct:state            = cppliability:state and
      cpsliabdeduct:line_of_business = cppliability:rating_line_of_business and
      cpsliabdeduct:deductible       = l_deductible and
      cpsliabdeduct:deductible_type  = l_deductible_type
        begin
        if sfpname:eff_date >= cpsliabdeduct:eff_date then
            begin
            switch(cppliability:limits_table)
              case 1  : l_deductible_factor_1 = cpsliabdeduct:factor[1]
              case 2  : l_deductible_factor_1 = cpsliabdeduct:factor[2]
              default : l_deductible_factor_1 = cpsliabdeduct:factor[3]
              end
            end

        next cpsliabdeduct
        end

end

