Procedure TOTALS
Begin
l_minimum_applies = "N"
if l_total < l_minimum then
    begin
    l_total = l_minimum
    l_minimum_applies = "Y"
    end

access cppliability_alias, set cppliability_alias:policy_no   = cppliability:policy_no,
                               cppliability_alias:pol_year = g_Year,
                               cppliability_alias:end_sequence = g_end_sequence, generic

while cppliability_alias:policy_no = cppliability:policy_no and
      cppliability_alias:pol_year = cppliability:pol_year and
      cppliability_alias:end_sequence = cppliability:end_sequence
    begin
    l_total_premise = l_total_premise + cppliability_alias:prem[1]
    if cppliability_alias:minimum_premium[1] > l_highest_prem_mp then
        l_highest_prem_mp = cppliability_alias:minimum_premium[1]
    l_total_products = l_total_products + cppliability_alias:prem[2]
    if cppliability_alias:minimum_premium[2] > l_highest_prod_mp then
        l_highest_prod_mp = cppliability_alias:minimum_premium[2]

    next cppliability_alias
    end

access cppliability_alias, set cppliability_alias:policy_no   = cppliability:policy_no,
                               cppliability_alias:pol_year = g_Year,
                               cppliability_alias:end_sequence = g_end_sequence, generic

l_mp_updated = "N"
while cppliability_alias:policy_no = cppliability:policy_no and
      cppliability_alias:pol_year = cppliability:pol_year and
      cppliability_alias:end_sequence = cppliability:end_sequence
    begin
    if cppliability_alias:base_rate[1] <> 0 and
       cppliability_alias:exposure[1] <> 0 and
       l_mp_updated = "N" then
        begin
        l_mp_updated = "Y"
        change cppliability_alias
            begin
            if l_no_products_mp = 0 then
                begin
                if l_total_premise < l_highest_prem_mp then
                    begin
                    cppliability_alias:minimum_premium_adjustment[1] = l_highest_prem_mp - l_total_premise
                    l_minimum_premium_adjustment_1 = l_highest_prem_mp - l_total_premise
                    end
                else
                    begin
                    cppliability_alias:minimum_premium_adjustment[1] = 0
                    l_minimum_premium_adjustment_1 = 0
                    end
                end
            else
            if l_no_products_mp = 1 then
                begin
                if (l_total_premise + l_total_products) < l_highest_prem_mp then
                    begin
                    cppliability_alias:minimum_premium_adjustment[1] = (l_highest_prem_mp -
                                                                       (l_total_premise +
                                                                       l_total_products))
                    l_minimum_premium_adjustment_1 = (l_highest_prem_mp -
                                                     (l_total_premise +
                                                     l_total_products))
                    end
                else
                    begin
                    cppliability_alias:minimum_premium_adjustment[1] = 0
                    l_minimum_premium_adjustment_1 = 0
                    end
                end

            if l_total_products < l_highest_prod_mp then
                begin
                cppliability_alias:minimum_premium_adjustment[2] = l_highest_prod_mp - l_total_products
                l_minimum_premium_adjustment_2 = l_highest_prod_mp - l_total_products
                end
            else
                begin
                cppliability_alias:minimum_premium_adjustment[2] = 0
                l_minimum_premium_adjustment_2 = 0
                end
            end
        end
    else
        begin
        change cppliability_alias
            begin
            cppliability_alias:minimum_premium_adjustment[1] = 0
            cppliability_alias:minimum_premium_adjustment[2] = 0
            end
        end

    if cppliability_alias:base_rate[1] = 0 then
        begin
        change cppliability_alias
            begin
            cppliability_alias:minimum_premium_adjustment[1] = 0
            cppliability_alias:minimum_premium_adjustment[2] = 0
            end
        end

    next cppliability_alias
    end

End

Procedure FINAL_TOTAL
Begin
L_EFF_DATE_1 = Dateadd(sfpNAME:EFF_DATE,0,1)
If sfpNAME:EXP_DATE < L_EFF_DATE_1 Then
    Begin
    L_DAYS_RATE = L_TOTAL Divide 365
    L_DAYS = sfpNAME:EXP_DATE - sfpNAME:EFF_DATE
    L_PRO_RATA_PREMIUM = L_DAYS * L_DAYS_RATE
    L_TOTAL = L_PRO_RATA_PREMIUM
    End

L_FINAL_TOTAL = L_TOTAL + L_BOILER_PREM_TOTAL

Access sfpMASTER, Set sfpMASTER:policy_NO = G_policy_NO,
                      sfpmaster:pol_year = g_year,
                      sfpmaster:end_sequence = g_end_sequence, Exact

If L_IRPM = 0 Then
    L_IRPM = 1

access sfssurcharge, set sfssurcharge:company_id       = cppliability:company_id,
                         sfssurcharge:state            = cppliability:state,
                         sfssurcharge:line_of_business = cppliability:line_of_business, generic

l_surcharge_date = 0
while sfssurcharge:company_id       = cppliability:company_id and
      sfssurcharge:state            = cppliability:state and
      sfssurcharge:line_of_business = cppliability:line_of_business
        begin
        if (sfpname:eff_date >= sfssurcharge:eff_date and
           (l_surcharge_date < sfssurcharge:eff_date or
           l_surcharge_date = 0)) then
            begin
            l_surcharge_date    = sfssurcharge:eff_date
            l_surcharge_factor  = sfssurcharge:surcharge_factor
            l_surcharge_minimum = sfssurcharge:minimum_premium
            end

        next sfssurcharge
        end

l_minimum_applies = "N"
If L_final_TOTAL <= L_MINIMUM Then
    Begin
    L_final_total = L_MINIMUM
    L_MINIMUM_APPLIES = "Y"
    end

L_SURCHARGE = L_FINAL_TOTAL * L_SURCHARGE_FACTOR
If L_SURCHARGE < L_SURCHARGE_MINIMUM Then
    L_SURCHARGE = L_SURCHARGE_MINIMUM

L_COMPANY_ID = cppliability:company_id
Access sfpMASTER, Set sfpMASTER:policy_NO = cppliability:policy_NO,
                      sfpmaster:pol_year = g_year,
                      sfpmaster:end_sequence = g_end_sequence, Exact

access sfsline, set sfsline:company_id       = sfpname:company_id,
                    sfsline:line_of_business = sfpname:line_of_business,
                    sfsline:lob_subline      = "00", generic

if sfsline:lob_code one of "CPORTFOLIO" then
    begin
    Access sfpmastsupp, Set sfpmastsupp:policy_NO = cppliability:policy_NO,
                            sfpmastsupp:pol_year = g_year,
                            sfpmastsupp:end_sequence = g_end_sequence,
                            sfpmastsupp:line_of_business = g_line_of_business, Exact

    If sfpmastsupp:policy_NO = cppliability:policy_NO and
       sfpmastsupp:pol_year = cppliability:pol_year and
       sfpmastsupp:end_sequence = cppliability:end_sequence and
       sfpmastsupp:line_of_business = g_line_of_business Then
        Change sfpmastsupp
            Begin
            sfpmastsupp:TOTAL[17] = L_TOTAL
            sfpmastsupp:TOTAL[18] = L_FINAL_TOTAL + l_terrorism_premium
            sfpmastsupp:TOTAL[20] = L_FINAL_TOTAL + L_SURCHARGE
            sfpmastsupp:AMT_DUE_TOTAL[20] = L_FINAL_TOTAL + L_SURCHARGE
            sfpmastsupp:TOTAL[5] = L_FINAL_TOTAL
            If L_MINIMUM_APPLIES = "Y" Then
                sfpmastsupp:MINIMUM_PREM_APPLIES = "Y"
            else
                sfpmastsupp:minimum_prem_applies = "N"
            sfpmastsupp:SURCHARGE = L_SURCHARGE
            sfpmastsupp:RAW_PREMIUM_AMT_DUE = l_total
            sfpmastsupp:terrorism_premium = l_terrorism_premium
            End
    end
else
    begin
    If sfpMASTER:policy_NO = cppliability:policy_NO and
       sfpmaster:pol_year = cppliability:pol_year and
       sfpmaster:end_sequence = cppliability:end_sequence Then
        Change sfpMASTER
            Begin
            sfpMASTER:TOTAL[17] = L_TOTAL
            sfpMASTER:TOTAL[18] = L_FINAL_TOTAL + l_terrorism_premium
            sfpMASTER:TOTAL[20] = L_FINAL_TOTAL + L_SURCHARGE
            sfpMASTER:AMT_DUE_TOTAL[20] = L_FINAL_TOTAL + L_SURCHARGE
            sfpMASTER:TOTAL[5] = L_FINAL_TOTAL
            sfpMASTER:INSTALLMENT_CHARGE = L_INSTALLMENT
            If L_MINIMUM_APPLIES = "Y" Then
                sfpMASTER:MINIMUM_PREM_APPLIES = "Y"
            else
                sfpmaster:minimum_prem_applies = "N"
            sfpMASTER:SURCHARGE = L_SURCHARGE
            sfpMASTER:RAW_PREMIUM_AMT_DUE = l_total
            sfpmaster:terrorism_premium = l_terrorism_premium
            End
    end

End

Procedure RATE_ENDORSEMENTS
Begin
Access SFSOPTEND, Set SFSOPTEND:COMPANY_ID       = sfpname:company_id,
                      SFSOPTEND:STATE            = cppliability:STATE,
                      SFSOPTEND:LINE_OF_BUSINESS = cppliability:rating_line_of_business,
                      SFSOPTEND:CODE             = L_CODE, Generic

while SFSOPTEND:COMPANY_ID       = sfpname:company_id and
      SFSOPTEND:STATE            = cppliability:STATE and
      SFSOPTEND:LINE_OF_BUSINESS = cppliability:rating_line_of_business and
      SFSOPTEND:CODE             =  L_CODE
    begin
    If (sfpNAME:EFF_DATE >= SFSOPTEND:EFF_DATE and
       (sfpname:eff_date <= sfsoptend:exp_date or
       sfsoptend:exp_date = 0)) Then
        Begin
        L_END_FACTOR   = SFSOPTEND:PREMIUM_1[1]
        L_END_LIMIT    = SFSOPTEND:LIMIT[1]
        L_END_PREMIUM1 = SFSOPTEND:PREMIUM_1[1]
        End

    if l_code one of "LMCG280" then
        begin
        access cppliability_alias, set cppliability_alias:policy_no    = sfpend:policy_no,
                                       cppliability_alias:pol_year     = sfpend:pol_year,
                                       cppliability_alias:end_sequence = sfpend:end_sequence,
                                       cppliability_alias:prem_no      = sfpend:prem_no,
                                       cppliability_alias:build_no     = sfpend:build_no, generic

        l_form_premium = 0
        while cppliability_alias:policy_no    = sfpend:policy_no and
              cppliability_alias:pol_year     = sfpend:pol_year and
              cppliability_alias:end_sequence = sfpend:end_sequence and
              cppliability_alias:prem_no      = sfpend:prem_no and
              cppliability_alias:build_no     = sfpend:build_no
            begin
            l_form_premium = l_form_premium +
                             cppliability_alias:prem[1]

            next cppliability_alias
            end

        l_end_premium1 = sfsoptend:premium_1[2]
        l_update_sfpend_premium = l_form_premium *
                                  l_end_factor
        if l_update_sfpend_premium < l_end_premium1 then
            l_update_sfpend_premium = l_end_premium1

        end

    Next SFSOPTEND
    End

End

End
