Access SFSLINE2,
   Set SFSLINE2:COMPANY_ID       = L_COMPANY_ID,
       SFSLINE2:LINE_OF_BUSINESS = SFPNAME:LINE_OF_BUSINESS,
       sfsline2:lob_subline      = "00", exact

